{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/companies/(.*)",
      "dest": "/api/companies/$1"
    },
    {
      "src": "/companies/([^/]+)",
      "dest": "/companies/[companyName]/page"
    }
  ]
}
